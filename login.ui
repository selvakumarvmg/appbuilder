   <string notr="true">/* =====================================================
GLOBAL BASE
===================================================== */
QWidget {
    background-color: #f2f2f2;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    color: #1f1f1f;
}

/* =====================================================
MAIN CONTAINER (NO CARD, NO FLOAT)
===================================================== */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #dcdcdc; 
    border-radius: 22px;
    padding: 10px;
box-shadow: 5px 5px 10px #888888;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 6px;
    margin: 10px 20px 15px 20px;
	
    font-size: 13px;
    font-weight: 600;
    color: #005fb8;
}

/* =====================================================
LABELS
===================================================== */
QLabel {
    background: transparent;
    font-size: 13px;
    color: #2b2b2b;
}

QLabel#label {
    font-size: 18px;
    font-weight: 600;
    color: #1f1f1f;
    qproperty-alignment: AlignCenter;
    margin-bottom: 12px;
}

/* =====================================================
INPUT FIELDS (REAL DESKTOP STYLE)
===================================================== */
QLineEdit {
    background-color: #ffffff;
    border: 1px solid #bdbdbd;
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 13px;
    color: #1f1f1f;
}

QLineEdit:focus {
    border: 1px solid #005fb8;
    background-color: #ffffff;
}

QLineEdit::placeholder {
    color: #7a7a7a;
}

/* Password field */
QLineEdit#passwordtxt {
    letter-spacing: 0.5px;
}

/* =====================================================
CHECKBOX / RADIO (NATIVE-LIKE)
===================================================== */
QCheckBox,
QRadioButton {
    spacing: 6px;
    font-size: 13px;
    color: #1f1f1f;
}

QCheckBox::indicator,
QRadioButton::indicator {
    width: 14px;
    height: 14px;
}

QCheckBox::indicator {
    border: 1px solid #7a7a7a;
    background: #ffffff;
}

QCheckBox::indicator:checked {
    background-color: #005fb8;
    border-color: #005fb8;
}

/* =====================================================
BUTTONS (REAL TOOL BUTTONS)
===================================================== */
QPushButton {
    background-color: #005fb8;
    color: #ffffff;
    border: 1px solid #005fb8;
    padding: 6px 16px;
    font-size: 13px;
    font-weight: 600;
    border-radius: 4px;
    min-width: 90px;
}

QPushButton:hover {
    background-color: #0a6fd6;
}

QPushButton:pressed {
    background-color: #004a92;
}

/* Secondary / Cancel */
QPushButton#btnCancel {
    background-color: #ffffff;
    color: #005fb8;
    border: 1px solid #bdbdbd;
}

QPushButton#btnCancel:hover {
    background-color: #f2f2f2;
}

/* =====================================================
DIALOG BUTTON BOX
===================================================== */
QDialogButtonBox QPushButton {
    min-width: 90px;
}
