   <string notr="true">/* ===============================
üåê Base Form Style (Clean/Neutral)
================================== */
QDialog, QWidget {
    background-color: #f4f6f8;  /* light neutral background */
    font-family: &quot;Segoe UI&quot;, &quot;Helvetica Neue&quot;, sans-serif;
    font-size: 14px;
    color: #333333;
}

/* ===============================
üñºÔ∏è QLabel Styling
================================== */
QLabel {
    background-color: transparent;
    font-size: 13px;
    color: #2e2e2e;
}

QLabel#label_2,
QLabel#label_3 {
    background-color: #e0e0e0;
    padding: 6px 10px;
    border-radius: 0px 5px 0px 5px;
    font-weight: 500;
    color: #1a1a1a;
}

QLabel#label {
    qproperty-alignment: AlignCenter;
    padding: 10px;
}

/* ===============================
üì¶ QGroupBox Styling
================================== */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #d1d1d1;
    border-radius: 10px;
    padding: 20px;
    margin-top: 10px;
    font-weight: bold;
}

QGroupBox::title {
    subcontrol-origin: content;
    subcontrol-position: top left;
    margin-top: -12px;
    padding: 4px 10px;
    font-size: 15px;
    color: #0078d7;
}

/* ===============================
üî§ QLineEdit (Inputs)
================================== */
QLineEdit {
    background-color: #ffffff;
    border: 1px solid #bfc5ca;
    border-radius: 6px;
    padding: 8px 12px;
    font-size: 13px;
    color: #333;
}

QLineEdit:focus {
    border: 1px solid #0078d7;
    background-color: #f0faff;
}

/* ===============================
‚úÖ QCheckBox with Clean Label Background
================================== */
QCheckBox {
    spacing: 6px;
    font-size: 13px;
    color: #1a1a1a;
    padding: 6px 8px;
    border-radius: 6px;
    background-color: #eeeeee;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 1px solid #999;
    background-color: #ffffff;
}

QCheckBox::indicator:checked {
    background-color: #0078d7;
    image: none;
}

QCheckBox::indicator:hover {
    border: 1px solid #3399ff;
}

/* ===============================
üîò QRadioButton Styling
================================== */
QRadioButton {
    spacing: 6px;
    font-size: 13px;
    color: #2e2e2e;
    padding: 6px 8px;
}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border-radius: 8px;
    border: 2px solid #0078d7;
    background-color: white;
}

QRadioButton::indicator:checked {
    background-color: #0078d7;
}

QRadioButton::indicator:unchecked {
    background-color: white;
    border: 2px solid #bbb;
}

/* ===============================
üß© QPushButton Styling (Flat &amp; Modern)
================================== */
QPushButton {
    background-color: #0078d7;
    color: white;
    padding: 8px 16px;
    border-radius: 5px;
    font-weight: bold;
    min-width: 90px;
    min-height: 30px;
    border: none;
}

QPushButton:hover {
    background-color: #005a9e;
}

QPushButton:pressed {
    background-color: #004578;
}

/* ===============================
üí¨ QDialogButtonBox Buttons
================================== */
QDialogButtonBox QPushButton {
    background-color: #0078d7;
    color: white;
    padding: 8px 16px;
    border-radius: 5px;
    font-weight: bold;
    border: none;
    min-width: 90px;
    min-height: 30px;
}

QDialogButtonBox QPushButton:hover {
    background-color: #005a9e;
}

QDialogButtonBox QPushButton:pressed {
    background-color: #004578;
}
